{
	"name": "pl_catsTrigger",
	"properties": {
		"activities": [
			{
				"name": "Set variable1",
				"type": "SetVariable",
				"dependsOn": [],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "varWorking",
					"value": {
						"value": "@concat(pipeline().parameters.pFolderPath,'xxx',pipeline().parameters.pFileName)",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"pFolderPath": {
				"type": "string",
				"defaultValue": "images"
			},
			"pFileName": {
				"type": "string",
				"defaultValue": "cat3.jpg"
			}
		},
		"variables": {
			"varWorking": {
				"type": "String"
			}
		},
		"annotations": []
	}
}